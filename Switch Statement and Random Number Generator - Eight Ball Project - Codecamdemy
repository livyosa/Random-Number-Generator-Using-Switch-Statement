let randomNumber = Math.floor(Math.random() * 7);

let eightBall = '';

switch(randomNumber){
  case 0:
    console.log(eightBall);
    break;
  case 2:
    console.log('2');
    break;
  case 3:
    console.log('3');
    break;
  case 4:
    console.log('mid');
    break;
  case 5:
    console.log('4');
    break;
  case 6:
    console.log('5');
    break;
  default:
    console.log('default');
    break;
}
